{
  "name": "start_app",
  "version": "1.1.0",
  "description": "Start app",
  "scripts": {
    "start": "NODE_PATH=. node server.js",
    "test": "karma start config/karma.conf.js",
    "lint": "tslint -c config/tslint.json \"src/app/**/*.ts\"",
    "pretest": "npm run lint",
    "build": "rimraf dist && webpack --config config/client_config/webpack.dev.js --progress --profile --bail --watch",
    "build_production": "rimraf dist && webpack --config config/client_config/webpack.prod.js --progress --profile --bail --watch",
    "extract": "ngx-translate-extract -p /**/*.pug -p /**/*.ts -p /**/*.html --input ./src/app --output ./src/assets/i18n/*.json --clean --format json"
  },
